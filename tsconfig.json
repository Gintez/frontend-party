{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": false,
    "declaration": false,
    "removeComments": true,
    "strictNullChecks": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "lib": ["es6", "dom"],
    "outDir": "dist",
    "baseUrl": "src"
  },
  "exclude": ["dist", "node_modules"]
}
